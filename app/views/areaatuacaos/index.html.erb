<p id="notice"><%= notice %></p>

<h1>Areaatuacaos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @areaatuacaos.each do |areaatuacao| %>
      <tr>
        <td><%= link_to 'Show', areaatuacao %></td>
        <td><%= link_to 'Edit', edit_areaatuacao_path(areaatuacao) %></td>
        <td><%= link_to 'Destroy', areaatuacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

