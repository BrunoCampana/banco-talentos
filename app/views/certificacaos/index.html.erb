<p id="notice"><%= notice %></p>

<h1>Certificacaos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @certificacaos.each do |certificacao| %>
      <tr>
        <td><%= link_to 'Show', certificacao %></td>
        <td><%= link_to 'Edit', edit_certificacao_path(certificacao) %></td>
        <td><%= link_to 'Destroy', certificacao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Certificacao', new_certificacao_path %>
